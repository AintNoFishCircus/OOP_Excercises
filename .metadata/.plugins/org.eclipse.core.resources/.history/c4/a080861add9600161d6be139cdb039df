
public class Fakultaet {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		System.out.println(berechneFak(5));
	}
	
	public static int berechneFak(int n){
		int fak;
		
		if (n < 0){
			return -1;
		}else if (n == 0){
			return 1;
		}else{
			for(fak = n; n > 0; n--){
				fak *= (n - 1);
			}
		}
		
		return fak;
	}

}
